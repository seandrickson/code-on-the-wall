{
  "name": "code-on-the-wall",
  "version": "1.0.0",
  "description": "Generate wallpaper from syntax highlighted, minified code",
  "main": "index.js",
  "author": "Sean Hendrickson",
  "license": "Apache-2.0",
  "scripts": {
    "setup": "yarn",
    "build": "webpack",
    "start": "node index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "puppeteer": "^1.10.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "yargs": "^12.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "no-console": 0
    }
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
